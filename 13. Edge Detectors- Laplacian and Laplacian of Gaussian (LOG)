subplot 211;
I3 = imread('cameraman.tif');
imshow(I3);
title('Original');
H = [[0,-1,0];[-1,4,-1];[0,-1,0]];
G = conv2(I3,H,'same');
subplot 212;
imshow(uint8(G));
title('Laplacian Edge Detection');
saveas(gcf,'LaplacianED.jpg');

subplot 221;
I3 = imread('cameraman.tif');
imshow(I3);
title('Original');
H = [[1,2,1];[2,4,2];[1,2,1]]/16;
G = conv2(I3,H,'same');
subplot 222;
imshow(uint8(G));
title('Gaussian');
subplot 223;
H1 = [[0,-1,0];[-1,4,-1];[0,-1,0]];
G1 = conv2(G,H1,'same');
imshow(uint8(G1));
title('Laplacian of Gaussian Edge Detection');
saveas(gcf,'LOGOP.jpg');
